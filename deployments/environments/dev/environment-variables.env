# DEV Environment Configuration
# AWS Account ID (leave empty to use current account)
DEV_AWS_ACCOUNT_ID=""

# External Service Endpoints (DEV instances)
DYNATRACE_API_URL="https://dev-tenant.dynatrace.com/api/v2"
DYNATRACE_API_TOKEN="your-dev-dynatrace-token"
SONARQUBE_URL="https://dev-sonarqube.your-domain.com"
SONARQUBE_TOKEN="your-dev-sonarqube-token"
WIZ_URL="https://dev-wiz.your-domain.com"
WIZ_CLIENT_ID="your-dev-wiz-client-id"
WIZ_CLIENT_SECRET="your-dev-wiz-client-secret"
SLACK_WEBHOOK_URL="https://hooks.slack.com/services/YOUR/DEV/WEBHOOK"

# LLM Configuration (DEV)
LLM_MODEL="codellama:7b"  # Smaller model for dev
LLM_TEMPERATURE="0.1"
LLM_MAX_TOKENS="2048"

# MCP Configuration (DEV)
MCP_SERVER_TYPE="mock"  # Use mock for dev environment
MCP_DYNATRACE_URL="https://dev-tenant.dynatrace.com/api/v2"

# Environment-specific settings
ENVIRONMENT_NAME="development"
LOG_LEVEL="DEBUG"
ENABLE_MOCK_SERVICES="true" 
# PROD Environment Configuration
# AWS Account ID (leave empty to use current account)
PROD_AWS_ACCOUNT_ID=""

# External Service Endpoints (PROD instances)
DYNATRACE_API_URL="https://prod-tenant.dynatrace.com/api/v2"
DYNATRACE_API_TOKEN="your-prod-dynatrace-token"
SONARQUBE_URL="https://prod-sonarqube.your-domain.com"
SONARQUBE_TOKEN="your-prod-sonarqube-token"
WIZ_URL="https://prod-wiz.your-domain.com"
WIZ_CLIENT_ID="your-prod-wiz-client-id"
WIZ_CLIENT_SECRET="your-prod-wiz-client-secret"
SLACK_WEBHOOK_URL="https://hooks.slack.com/services/YOUR/PROD/WEBHOOK"

# LLM Configuration (PROD)
LLM_MODEL="codellama:13b"  # Full model for production
LLM_TEMPERATURE="0.1"
LLM_MAX_TOKENS="4096"

# MCP Configuration (PROD)
MCP_SERVER_TYPE="dynatrace"  # Use real Dynatrace for production
MCP_DYNATRACE_URL="https://prod-tenant.dynatrace.com/api/v2"

# Environment-specific settings
ENVIRONMENT_NAME="production"
LOG_LEVEL="WARN"
ENABLE_MOCK_SERVICES="false" 